 <div class='row'>
  <div class='col-md-12'>
     <table id="program_table" class='display dataTable table table-condensed table-hover table-bordered'>
        <thead>
          <tr>
            <th>#</th>
            <th>Name</th>
            <th>PID</th>
            <th>Type</th>
            <th>Start</th>
            <th>Slot</th>
            <th>State</th>
          </tr>
        </thead>
        <tbody>
          <% @programs.each do |program| %>
          <tr>
            <td><%= program.id %></td>
            <td><%= link_to program.name, program_path(program)%></td>
            <td><%= program.pid %></td>
            <td><%= (program.program_donation.name rescue '')%></td>
            <td><%= program.start_date.strftime('%d %B %Y') %></td>
            <td><%= program.display_timings %></td>
            <td><%= program_state_tag(program) %></td>
          </tr>
          <% end %>
        </tbody>
    </table>
  </div>
</div>