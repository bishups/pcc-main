<% timings =  @selected_timings %>
$('#timing_ids_select').html("<%= escape_javascript(options_for_select(timings.map {|e| [e.name, e.id]})) %>");
<% program_timing_str = @selected_program.blank? ? "" : @selected_program.display_timings %>
$('#program_timings_label').html("<%= escape_javascript(program_timing_str) %>");


